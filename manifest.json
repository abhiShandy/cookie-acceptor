{
    "manifest_version": 3,
    "name": "Cookie Prompt Acceptor",
    "version": "1.1",
    "description": "Automatically accepts cookie prompts on websites",
    "permissions": [],
    "content_scripts": [
      {
        "matches": [
          "https://*/*",
          "https://*"
        ],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icon128.png"
    }
  }