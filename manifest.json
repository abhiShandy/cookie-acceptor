{
    "manifest_version": 3,
    "name": "Cookie Prompt Acceptor",
    "version": "1.0",
    "description": "Automatically accepts cookie prompts on certain websites",
    "permissions": [],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.stackoverflow.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icon128.png"
    }
  }